{
  "name": "virtualdeck",
  "description": "A virtual control pad",
  "version": "0.0.0",
  "author": "iamtheluiz <iamtheluiz.dev@gmail.com>",
  "repository": "https://github.com/iamtheluiz/deck.git",
  "private": true,
  "main": "main.js",
  "build": {
    "appId": "com.electron.virtualdeck",
    "win": {
      "target": "NSIS",
      "publish": ["github"]
    }
  },
  "scripts": {
    "start": "electron .",
    "rebuild": "npm rebuild --runtime=electron --target=1.1.3 --disturl=https://atom.io/download/atom-shell --abi=48",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "opener": "^1.5.1",
    "robotjs": "^0.6.0"
  },
  "devDependencies": {
    "electron": "^8.2.2",
    "electron-builder": "^22.4.1",
    "electron-rebuild": "^1.10.1",
    "nodemon": "^2.0.3"
  }
}
